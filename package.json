{
  "name": "@iludolf/embedding-cli",
  "version": "0.1.0",
  "description": "CLI for estimating embedding costs and syncing PostgreSQL tables to Elasticsearch with vector search",
  "main": "src/embedding-cli.ts",
  "bin": {
    "embedding-cli": "./bin/embedding-cli.js"
  },
  "files": [
    "bin/",
    "src/embedding-cli.ts",
    "src/gather-embedding-responses.ts",
    "src/embedding-types.ts",
    "src/helpers.ts",
    "src/logger.ts",
    "src/constants.ts",
    "cost_estimator/",
    "db_sync_graph/",
    "shared/",
    "tsconfig.json",
    "tsconfig.embedding.json"
  ],
  "keywords": [
    "embedding",
    "postgresql",
    "elasticsearch",
    "openai",
    "cost-estimator",
    "vector-search",
    "langchain",
    "cli"
  ],
  "author": "Israel Ludolf",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "typecheck": "tsc --project tsconfig.embedding.json --noEmit",
    "dev": "langgraphjs dev",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.test\\.ts$ --testPathIgnorePatterns=\\.int\\.test\\.ts$",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.int\\.test\\.ts$",
    "estimate-cost": "tsx src/cost_estimator/estimate.ts",
    "embedding-cli": "tsx src/embedding-cli.ts",
    "lint": "eslint src cost_estimator db_sync_graph shared",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@elastic/elasticsearch": "^8.15.0",
    "@langchain/anthropic": "^0.3.1",
    "@langchain/cohere": "^0.3.0",
    "@langchain/community": "^0.3.1",
    "@langchain/core": "^0.3.59",
    "@langchain/langgraph": "^0.4.0",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.3.0",
    "@langchain/pinecone": "^0.1.0",
    "@pinecone-database/pinecone": "^3.0.3",
    "dotenv": "^16.4.5",
    "js-tiktoken": "^1.0.14",
    "langchain": "^0.3.2",
    "mongodb": "^6.9.0",
    "pg": "^8.18.0",
    "picocolors": "^1.1.1",
    "semver": "^7.7.4",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@langchain/langgraph-cli": "^1.1.12",
    "@tsconfig/recommended": "^1.0.7",
    "@types/jest": "^29.5.0",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
